{
	"configurations": [
		{
			"args": ["${relativeFile}"],
			"cwd": "${workspaceRoot}",
			"env": {
				"NODE_ENV": "test"
			},
			"envFile": "${workspaceFolder}/.env.local",
			"internalConsoleOptions": "openOnSessionStart",
			"name": "ts-node",
			"protocol": "inspector",
			"request": "launch",
			"runtimeArgs": ["-r", "ts-node/register"],
			"skipFiles": ["<node_internals>/**"],
			"type": "node"
		}
	],
	"version": "0.2.0"
}
