{
	"name": "lsg",
	"version": "0.0.0",
	"description": "LSG musin fullstack",
	"repository": {
		"type": "git",
		"url": "https://github.com/3x071c/lsg"
	},
	"homepage": "https://lsg.musin.de",
	"bugs": {
		"url": "https://github.com/3x071c/lsg/issues"
	},
	"author": {
		"email": "dev@3x071c.com",
		"name": "Victor Homic",
		"url": "https://3x071c.com"
	},
	"license": "AGPL-3.0-or-later",
	"private": true,
	"engines": {
		"node": ">= 14"
	},
	"os": [
		"linux",
		"darwin",
		"win64",
		"win32"
	],
	"scripts": {
		"build": "cross-env NODE_ENV=production npm run remix:build",
		"check": "tsc --noEmit",
		"ci": "run-s generate lint check",
		"clean": "run-s \"clean:*\"",
		"clean:1": "rimraf .remix",
		"clean:2": "rimraf public/build",
		"commit": "run-s stage cz",
		"cspell:individual": "cspell",
		"cspell:report": "npm run cspell:individual -- \"**\"",
		"cz": "cz",
		"dev": "npm run development",
		"development": "cross-env NODE_ENV=development npm run remix:dev",
		"eslint:fix": "npm run eslint:template -- --fix .",
		"eslint:individual": "npm run eslint:template -- --fix",
		"eslint:report": "npm run eslint:template -- --max-warnings 0 .",
		"eslint:template": "eslint --cache --report-unused-disable-directives --no-error-on-unmatched-pattern --ext .tsx,.ts,.jsx,.js,.mjs,.cjs",
		"fix": "npm run format",
		"format": "run-s prettier:fix eslint:fix secretlint:report cspell:report jscpd:report",
		"generate": "npm run prisma:generate",
		"jscpd:individual": "jscpd",
		"jscpd:report": "npm run jscpd:individual -- \".\"",
		"lint": "run-s prettier:report eslint:report secretlint:report cspell:report jscpd:report",
		"new": "npm run new:dev",
		"new:dev": "run-s generate prisma:reset prisma:dev prisma:seed",
		"new:preview": "run-s prisma:reset prisma:prod prisma:seed",
		"new:prod": "npm run prisma:prod",
		"postinstall": "remix setup node",
		"preinstall": "npx --yes -- only-allow npm",
		"prepare": "husky install",
		"prettier:fix": "npm run prettier:individual -- .",
		"prettier:individual": "npm run prettier:template -- --write",
		"prettier:report": "npm run prettier:template -- --check .",
		"prettier:template": "prettier --plugin-search-dir=. --ignore-unknown",
		"prisma:dev": "prisma migrate dev --skip-generate --skip-seed",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev --skip-generate --skip-seed --name",
		"prisma:prod": "prisma migrate deploy",
		"prisma:push": "prisma db push --skip-generate",
		"prisma:reset": "prisma migrate reset --skip-generate --skip-seed --force",
		"prisma:seed": "prisma db seed",
		"prisma:studio": "prisma studio",
		"prisma:watch": "npm run prisma:generate -- --watch",
		"prod": "npm run production",
		"production": "cross-env NODE_ENV=production run-s build start",
		"railway:start": "if [ \"$RAILWAY_GIT_BRANCH\" = \"trunk\" ]; then npm run new:prod; else npm run new:preview; fi; npm run start",
		"remix:build": "remix build",
		"remix:dev": "remix dev",
		"remix:start": "remix-serve .remix/server",
		"report": "npm run lint",
		"secretlint:individual": "secretlint",
		"secretlint:report": "npm run secretlint:individual -- \".\"",
		"stage": "git add -A",
		"start": "cross-env NODE_ENV=production npm run remix:start"
	},
	"dependencies": {
		"@chakra-ui/icons": "1.1.7",
		"@chakra-ui/react": "1.8.5",
		"@chakra-ui/system": "1.11.1",
		"@chakra-ui/theme-tools": "1.3.6",
		"@emotion/cache": "11.7.1",
		"@emotion/react": "11.8.1",
		"@emotion/server": "11.4.0",
		"@emotion/styled": "11.8.1",
		"@emotion/utils": "1.1.0",
		"@prisma/client": "3.10.0",
		"@remix-run/react": "1.2.3",
		"@remix-run/serve": "1.2.3",
		"@remix-validated-form/with-zod": "^2.0.1",
		"argon2": "0.28.4",
		"framer-motion": "6.2.8",
		"lodash": "4.17.21",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-icons": "4.3.1",
		"react-table": "7.7.0",
		"remix": "1.2.3",
		"sharp": "0.30.1",
		"zod-form-data": "^1.2.0"
	},
	"devDependencies": {
		"@babel/core": "7.17.5",
		"@commitlint/cli": "16.2.1",
		"@commitlint/config-conventional": "16.2.1",
		"@cspell/dict-de-de": "2.1.0",
		"@faker-js/faker": "6.0.0-alpha.7",
		"@remix-run/dev": "1.2.3",
		"@remix-run/eslint-config": "1.2.3",
		"@secretlint/secretlint-rule-preset-recommend": "4.1.4",
		"@swc/core": "1.2.154",
		"@types/faker": "5.5.9",
		"@types/lodash": "4.14.179",
		"@types/node": "17.0.21",
		"@types/react": "17.0.39",
		"@types/react-dom": "17.0.13",
		"@types/sharp": "0.30.0",
		"@typescript-eslint/eslint-plugin": "5.12.1",
		"@typescript-eslint/parser": "5.12.1",
		"browserslist": "4.19.3",
		"commitizen": "4.2.4",
		"cross-env": "7.0.3",
		"cspell": "5.18.5",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "8.10.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "16.1.0",
		"eslint-config-prettier": "8.4.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.29.2",
		"eslint-plugin-react-hooks": "4.3.0",
		"faker": "5.5.3",
		"husky": "7.0.4",
		"jscpd": "3.4.5",
		"lint-staged": "12.3.4",
		"npm-run-all": "4.1.5",
		"only-allow": "1.1.0",
		"prettier": "2.5.1",
		"prettier-plugin-pkg": "0.11.1",
		"prettier-plugin-prisma": "3.10.0",
		"prettier-plugin-sh": "0.8.1",
		"prettier-plugin-sort-json": "0.0.2",
		"prisma": "3.10.0",
		"rimraf": "3.0.2",
		"secretlint": "4.1.4",
		"ts-node": "10.5.0",
		"tsconfig-paths": "3.12.0",
		"typescript": "4.5.5"
	},
	"sideEffects": false,
	"browserslist": [
		"defaults"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"npm run prettier:individual",
			"npm run secretlint:individual",
			"npm run cspell:individual",
			"npm run jscpd:individual"
		],
		"*.{tsx,ts,jsx,js,mjs,cjs}": "npm run eslint:individual"
	},
	"prisma": {
		"schema": "db/schema.prisma",
		"seed": "ts-node db/seed.ts"
	}
}
