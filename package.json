{
	"name": "lsg-stack",
	"version": "0.0.0",
	"description": "LSG musin fullstack",
	"repository": {
		"type": "git",
		"url": "https://github.com/3x071c/lsg-stack"
	},
	"homepage": "https://lsg.musin.de",
	"bugs": {
		"url": "https://github.com/3x071c/lsg-stack/issues"
	},
	"author": {
		"email": "dev@3x071c.com",
		"name": "Victor Homic",
		"url": "https://3x071c.com"
	},
	"license": "AGPL-3.0-or-later",
	"private": true,
	"engines": {
		"node": ">= 14"
	},
	"os": [
		"linux",
		"darwin",
		"win64",
		"win32"
	],
	"scripts": {
		"apollo:generate": "apollo client:codegen --includes=src/** --localSchemaFile=__generated__/schema.graphql --outputFlat --tagName=gql --target=typescript --tsFileExtension=d.ts --useReadOnlyTypes __generated__/apollo.d.ts",
		"apollo:watch": "apollo client:codegen --includes=src/** --localSchemaFile=__generated__/schema.graphql --outputFlat --tagName=gql --target=typescript --tsFileExtension=d.ts --useReadOnlyTypes --watch __generated__/apollo.d.ts",
		"build": "cross-env NODE_ENV=production run-s clean generate next:build",
		"check": "tsc --noEmit || echo 'Type checking failed, skipping due to blocking issue in chakra-ui' && true",
		"ci": "run-s new lint check build",
		"clean": "run-s \"clean:*\"",
		"clean:1": "rimraf .next",
		"commit": "run-s stage cz",
		"cspell:individual": "cspell",
		"cspell:report": "pnpm cspell:individual -- \"**\"",
		"cz": "cz",
		"dev": "pnpm development",
		"development": "cross-env NODE_ENV=development run-s clean generate next:dev",
		"eslint:fix": "pnpm eslint:template -- --fix .",
		"eslint:individual": "pnpm eslint:template -- --fix",
		"eslint:report": "pnpm eslint:template -- --max-warnings 0 .",
		"eslint:template": "eslint --cache --report-unused-disable-directives --no-error-on-unmatched-pattern --ext .tsx,.ts,.jsx,.js,.mjs,.cjs",
		"export": "cross-env NODE_ENV=production run-s clean next:export",
		"fix": "pnpm format",
		"format": "run-s prettier:fix eslint:fix stylelint:fix secretlint:report cspell:report jscpd:report prisma:format",
		"generate": "run-s prisma:generate nexus:generate apollo:generate",
		"jscpd:individual": "jscpd",
		"jscpd:report": "pnpm jscpd:individual -- \".\"",
		"lint": "run-s prettier:report eslint:report stylelint:report secretlint:report cspell:report jscpd:report prisma:format",
		"new": "run-s clean prisma:reset prisma:push generate prisma:seed",
		"next:build": "next build",
		"next:dev": "next dev",
		"next:export": "next export",
		"next:start": "next start",
		"nexus:generate": "cross-env NODE_ENV=development ts-node --transpile-only --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" src/graphql/schema",
		"nexus:watch": "cross-env NODE_ENV=development ts-node-dev --transpile-only --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" src/graphql/schema",
		"preinstall": "pnpx -y only-allow pnpm",
		"prepare": "pnpx -y husky install",
		"prettier:fix": "pnpm prettier:individual -- .",
		"prettier:individual": "pnpm prettier:template -- --write",
		"prettier:report": "pnpm prettier:template -- --check .",
		"prettier:template": "prettier --plugin-search-dir=. --ignore-unknown",
		"prisma:deploy": "prisma migrate deploy",
		"prisma:format": "prisma format",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev --skip-generate --skip-seed --name \"name\"",
		"prisma:push": "prisma db push --skip-generate",
		"prisma:reset": "prisma migrate reset --skip-generate --skip-seed --force",
		"prisma:seed": "prisma db seed",
		"prisma:studio": "prisma studio",
		"prisma:watch": "pnpm prisma:generate -- --watch",
		"prod": "pnpm production",
		"production": "cross-env NODE_ENV=production run-s build start",
		"pull": "git pull --all --rebase",
		"push": "git push --prune -u origin HEAD",
		"report": "pnpm lint",
		"secretlint:individual": "secretlint",
		"secretlint:report": "pnpm secretlint:individual -- \".\"",
		"stage": "git add -A",
		"start": "cross-env NODE_ENV=production pnpm next:start",
		"stylelint:fix": "pnpm stylelint:template -- --fix \"./**/*.{tsx,jsx,html,pcss,css}\"",
		"stylelint:individual": "pnpm stylelint:template -- --fix",
		"stylelint:report": "pnpm stylelint:template -- --max-warnings 0 \"./**/*.{tsx,jsx,html,pcss,css}\"",
		"stylelint:template": "stylelint --cache --report-needless-disables --report-invalid-scope-disables --report-descriptionless-disables",
		"watch": "run-p apollo:watch prisma:watch"
	},
	"dependencies": {
		"@apollo/client": "^3.5.8",
		"@chakra-ui/icons": "^1.1.1",
		"@chakra-ui/react": "^1.8.1",
		"@chakra-ui/system": "^1.10.1",
		"@emotion/cache": "^11.7.1",
		"@emotion/css": "^11.7.1",
		"@emotion/react": "^11.7.1",
		"@emotion/server": "^11.4.0",
		"@emotion/styled": "^11.6.0",
		"@prisma/client": "^3.8.1",
		"apollo-server-micro": "^3.6.2",
		"argon2": "^0.28.3",
		"framer-motion": "^5.6.0",
		"graphql": "^15.8.0",
		"graphql-scalars": "^1.14.1",
		"iron-session": "^6.0.5",
		"lodash": "^4.17.21",
		"micro": "^9.3.4",
		"micro-cors": "^0.1.1",
		"next": "^12.0.9",
		"nexus": "^1.2.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-hook-form": "^7.25.1",
		"react-icons": "^4.3.1",
		"react-table": "^7.7.0",
		"sharp": "^0.29.3",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@babel/core": "^7.16.12",
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@cspell/dict-de-de": "^2.1.0",
		"@faker-js/faker": "^6.0.0-alpha.5",
		"@secretlint/secretlint-rule-preset-recommend": "^4.1.4",
		"@stylelint/postcss-css-in-js": "^0.37.2",
		"@types/faker": "5.5.3",
		"@types/lodash": "^4.14.178",
		"@types/micro": "^7.3.6",
		"@types/micro-cors": "^0.1.2",
		"@types/node": "^17.0.13",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"apollo": "^2.33.9",
		"autoprefixer": "^10.4.2",
		"browserslist": "^4.19.1",
		"commitizen": "^4.2.4",
		"cross-env": "^7.0.3",
		"cspell": "^5.17.0",
		"cssnano": "^5.0.16",
		"cssnano-preset-advanced": "^5.1.11",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.8.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^16.1.0",
		"eslint-config-next": "^12.0.9",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"faker": "5.5.3",
		"husky": "^7.0.4",
		"jscpd": "^3.4.5",
		"lint-staged": "^12.3.2",
		"npm-run-all": "^4.1.5",
		"only-allow": "^1.0.0",
		"postcss": "^8.4.5",
		"postcss-fail-on-warn": "^0.2.1",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-import": "^14.0.2",
		"postcss-preset-env": "^7.2.3",
		"postcss-syntax": "^0.36.2",
		"prettier": "^2.5.1",
		"prettier-plugin-pkg": "^0.11.1",
		"prettier-plugin-prisma": "^3.8.0",
		"prettier-plugin-sh": "^0.8.1",
		"prettier-plugin-sort-json": "^0.0.2",
		"prisma": "^3.8.1",
		"rimraf": "^3.0.2",
		"secretlint": "^4.1.4",
		"stylelint": "^14.3.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recommended": "^6.0.0",
		"stylelint-config-standard": "^24.0.0",
		"ts-node": "^10.4.0",
		"ts-node-dev": "^1.1.8",
		"tsconfig-paths": "^3.12.0",
		"typescript": "^4.5.5",
		"webpack": "^5.67.0"
	},
	"browserslist": [
		"defaults"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"pnpm prettier:individual",
			"pnpm secretlint:individual",
			"pnpm cspell:individual",
			"pnpm jscpd:individual"
		],
		"*.prisma": "pnpm prisma:format -- --schema",
		"*.{tsx,jsx,html,pcss,css}": "pnpm stylelint:individual",
		"*.{tsx,ts,jsx,js,mjs,cjs}": "pnpm eslint:individual"
	},
	"prisma": {
		"schema": "src/graphql/schema.prisma",
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/graphql/seed.ts"
	}
}
