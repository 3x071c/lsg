{
	"name": "lsg",
	"version": "0.0.0",
	"description": "LSG musin fullstack",
	"repository": {
		"type": "git",
		"url": "https://github.com/3x071c/lsg"
	},
	"homepage": "https://lsg.musin.de",
	"bugs": {
		"url": "https://github.com/3x071c/lsg/issues"
	},
	"author": {
		"email": "dev@3x071c.com",
		"name": "Victor Homic",
		"url": "https://3x071c.com"
	},
	"license": "AGPL-3.0-or-later",
	"private": true,
	"engines": {
		"node": ">= 16"
	},
	"os": [
		"linux",
		"darwin",
		"win64",
		"win32"
	],
	"scripts": {
		"build": "cross-env NODE_ENV=production npm run remix:build",
		"check": "tsc --noEmit",
		"clean": "run-s \"clean:*\"",
		"clean:1": "rimraf .remix",
		"clean:2": "rimraf public/build",
		"commit": "run-s stage cz",
		"cspell:individual": "cspell",
		"cspell:report": "npm run cspell:individual -- \"**\"",
		"cz": "cz",
		"dev": "npm run development",
		"development": "cross-env NODE_ENV=development npm run remix:build && run-p remix:dev wrangler:dev",
		"eslint:fix": "npm run eslint:template -- --fix .",
		"eslint:individual": "npm run eslint:template -- --fix",
		"eslint:report": "npm run eslint:template -- --max-warnings 0 .",
		"eslint:template": "eslint --cache --report-unused-disable-directives --no-error-on-unmatched-pattern --ext .tsx,.ts,.jsx,.js,.mjs,.cjs",
		"fix": "npm run format",
		"format": "run-s prettier:fix eslint:fix secretlint:report cspell:report jscpd:report",
		"jscpd:individual": "jscpd",
		"jscpd:report": "npm run jscpd:individual -- \".\"",
		"lint": "run-s prettier:report eslint:report secretlint:report cspell:report jscpd:report",
		"postinstall": "remix setup cloudflare-pages",
		"preinstall": "npx --yes -- only-allow npm",
		"prepare": "husky install",
		"prettier:fix": "npm run prettier:individual -- .",
		"prettier:individual": "npm run prettier:template -- --write",
		"prettier:report": "npm run prettier:template -- --check .",
		"prettier:template": "prettier --plugin-search-dir=. --ignore-unknown",
		"prod": "npm run production",
		"production": "cross-env NODE_ENV=production run-s build start",
		"remix:build": "remix build",
		"remix:dev": "remix watch",
		"report": "npm run lint",
		"secretlint:individual": "secretlint",
		"secretlint:report": "npm run secretlint:individual -- \".\"",
		"stage": "git add -A",
		"start": "cross-env NODE_ENV=production npm run wrangler:dev",
		"wrangler:dev": "wrangler pages dev ./public"
	},
	"dependencies": {
		"@chakra-ui/icons": "1.1.7",
		"@chakra-ui/react": "1.8.6",
		"@chakra-ui/system": "1.11.2",
		"@chakra-ui/theme-tools": "1.3.6",
		"@emotion/cache": "11.7.1",
		"@emotion/react": "11.8.2",
		"@emotion/server": "11.4.0",
		"@emotion/styled": "11.8.1",
		"@emotion/utils": "1.1.0",
		"@remix-run/cloudflare-pages": "1.3.2",
		"@remix-run/react": "1.3.2",
		"@remix-validated-form/with-zod": "2.0.1",
		"argon2": "0.28.5",
		"framer-motion": "6.2.8",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-icons": "4.3.1",
		"react-table": "7.7.0",
		"remix": "1.3.2",
		"remix-validated-form": "4.1.8",
		"superjson": "1.8.1",
		"zod": "3.14.2",
		"zod-form-data": "1.2.0"
	},
	"devDependencies": {
		"@babel/core": "7.17.8",
		"@cloudflare/workers-types": "3.4.0",
		"@commitlint/cli": "16.2.3",
		"@commitlint/config-conventional": "16.2.1",
		"@cspell/dict-de-de": "2.1.0",
		"@remix-run/dev": "1.3.2",
		"@remix-run/eslint-config": "1.3.2",
		"@secretlint/secretlint-rule-preset-recommend": "5.1.1",
		"@swc/core": "1.2.160",
		"@types/node": "17.0.23",
		"@types/react": "17.0.42",
		"@types/react-dom": "17.0.14",
		"@typescript-eslint/eslint-plugin": "5.16.0",
		"@typescript-eslint/parser": "5.16.0",
		"browserslist": "4.20.2",
		"commitizen": "4.2.4",
		"cross-env": "7.0.3",
		"cspell": "5.19.3",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "8.11.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "16.1.4",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.29.4",
		"eslint-plugin-react-hooks": "4.3.0",
		"husky": "7.0.4",
		"jscpd": "3.4.5",
		"lint-staged": "12.3.7",
		"npm-run-all": "4.1.5",
		"only-allow": "1.1.0",
		"prettier": "2.6.0",
		"prettier-plugin-pkg": "0.11.1",
		"prettier-plugin-sh": "0.8.1",
		"prettier-plugin-sort-json": "0.0.2",
		"rimraf": "3.0.2",
		"secretlint": "5.1.1",
		"ts-node": "10.7.0",
		"tsconfig-paths": "3.14.1",
		"typescript": "4.6.2",
		"wrangler": "beta"
	},
	"sideEffects": false,
	"browserslist": [
		"defaults"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"npm run prettier:individual",
			"npm run secretlint:individual",
			"npm run cspell:individual",
			"npm run jscpd:individual"
		],
		"*.{tsx,ts,jsx,js,mjs,cjs}": "npm run eslint:individual"
	}
}
