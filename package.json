{
	"name": "lsg",
	"version": "0.0.0",
	"description": "LSG musin fullstack",
	"repository": {
		"type": "git",
		"url": "https://github.com/3x071c/lsg"
	},
	"homepage": "https://lsg.musin.de",
	"bugs": {
		"url": "https://github.com/3x071c/lsg/issues"
	},
	"author": {
		"email": "dev@3x071c.com",
		"name": "Victor Homic",
		"url": "https://3x071c.com"
	},
	"license": "AGPL-3.0-or-later",
	"private": true,
	"engines": {
		"node": ">= 16"
	},
	"os": [
		"linux",
		"darwin"
	],
	"scripts": {
		"build": "cross-env NODE_ENV=production run-s clean remix:build",
		"check": "tsc --noEmit",
		"clean": "run-s \"clean:*\"",
		"clean:1": "rimraf .cache",
		"clean:2": "rimraf functions",
		"clean:3": "rimraf build",
		"clean:4": "rimraf public/build",
		"commit": "run-s stage cz",
		"cspell:individual": "cspell",
		"cspell:report": "npm run cspell:individual -- \"**\"",
		"cz": "cz",
		"dev": "npm run development",
		"development": "cross-env NODE_ENV=development run-s clean remix:build && run-p remix:dev wrangler:dev",
		"eslint:fix": "npm run eslint:template -- --fix .",
		"eslint:individual": "npm run eslint:template -- --fix",
		"eslint:report": "npm run eslint:template -- --max-warnings 0 .",
		"eslint:template": "eslint --cache --report-unused-disable-directives --no-error-on-unmatched-pattern --ext .tsx,.ts,.jsx,.js,.mjs,.cjs",
		"fix": "npm run format",
		"format": "run-s prettier:fix eslint:fix secretlint:report cspell:report jscpd:report",
		"jscpd:individual": "jscpd",
		"jscpd:report": "npm run jscpd:individual -- \".\"",
		"lint": "run-s prettier:report eslint:report secretlint:report cspell:report jscpd:report",
		"postinstall": "remix setup cloudflare && remix-esbuild-override && npm run clean",
		"preinstall": "npm exec --yes -- only-allow npm",
		"prepare": "husky install",
		"prettier:fix": "npm run prettier:individual -- .",
		"prettier:individual": "npm run prettier:template -- --write",
		"prettier:report": "npm run prettier:template -- --check .",
		"prettier:template": "prettier --plugin-search-dir=. --ignore-unknown",
		"remix:build": "remix build",
		"remix:dev": "remix watch",
		"report": "npm run lint",
		"secretlint:individual": "secretlint",
		"secretlint:report": "npm run secretlint:individual -- \".\"",
		"stage": "git add -A",
		"wrangler:dev": "env-cmd sh -c 'wrangler pages dev ./public --kv users --kv pages --kv pageGroups --binding MAGIC_KEY=\"$MAGIC_KEY\" --binding MAGIC_SECRET=\"$MAGIC_SECRET\" --binding CMS_AUTH_SECRET=\"$CMS_AUTH_SECRET\" --binding IMAGEDELIVERY=\"$IMAGEDELIVERY\"'"
	},
	"dependencies": {
		"@chakra-ui/icons": "1.1.7",
		"@chakra-ui/react": "1.8.7",
		"@chakra-ui/system": "1.12.0",
		"@chakra-ui/theme-tools": "1.3.6",
		"@charlietango/use-native-lazy-loading": "1.9.0",
		"@emotion/cache": "11.7.1",
		"@emotion/react": "11.8.2",
		"@emotion/server": "11.4.0",
		"@emotion/styled": "11.8.1",
		"@emotion/utils": "1.1.0",
		"@magic-sdk/admin": "1.4.0",
		"@remix-run/cloudflare-pages": "1.3.5",
		"@remix-run/react": "1.3.5",
		"@remix-validated-form/with-zod": "2.0.1",
		"cross-env": "7.0.3",
		"env-cmd": "10.1.0",
		"framer-motion": "6.2.8",
		"magic-sdk": "8.1.0",
		"npm-run-all": "4.1.5",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-focus-lock": "2.8.1",
		"react-hook-form": "7.28.1",
		"react-icons": "4.3.1",
		"react-intersection-observer": "8.33.1",
		"react-table": "7.7.0",
		"react-window": "1.8.6",
		"remix": "1.3.5",
		"remix-validated-form": "4.1.8",
		"superjson": "1.8.1",
		"zod": "3.14.3"
	},
	"devDependencies": {
		"@babel/core": "7.17.8",
		"@cloudflare/workers-types": "3.4.0",
		"@commitlint/cli": "16.2.3",
		"@commitlint/config-conventional": "16.2.1",
		"@cspell/dict-de-de": "2.1.0",
		"@esbuild-plugins/node-globals-polyfill": "0.1.1",
		"@remix-run/dev": "1.3.5",
		"@remix-run/eslint-config": "1.3.5",
		"@secretlint/secretlint-rule-preset-recommend": "5.1.1",
		"@types/react": "17.0.44",
		"@types/react-dom": "17.0.15",
		"@types/react-table": "7.7.10",
		"@types/react-window": "1.8.5",
		"@typescript-eslint/eslint-plugin": "5.17.0",
		"@typescript-eslint/parser": "5.17.0",
		"browserslist": "4.20.2",
		"commitizen": "4.2.4",
		"cspell": "5.19.3",
		"cz-conventional-changelog": "3.3.0",
		"esbuild-plugin-alias": "0.2.1",
		"eslint": "8.12.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "16.1.4",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.29.4",
		"eslint-plugin-react-hooks": "4.4.0",
		"husky": "7.0.4",
		"jscpd": "3.4.5",
		"no-op": "1.0.3",
		"only-allow": "1.1.0",
		"prettier": "2.6.1",
		"prettier-plugin-pkg": "0.11.1",
		"prettier-plugin-sh": "0.8.1",
		"prettier-plugin-sort-json": "0.0.2",
		"remix-esbuild-override": "3.0.2",
		"rimraf": "3.0.2",
		"secretlint": "5.1.1",
		"typescript": "4.6.3",
		"wrangler": "0.0.25"
	},
	"sideEffects": false,
	"browserslist": [
		"defaults"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	}
}
