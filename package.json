{
	"name": "lsg-stack",
	"version": "0.0.0",
	"description": "LSG musin fullstack",
	"repository": {
		"type": "git",
		"url": "https://github.com/3x071c/lsg-stack"
	},
	"homepage": "https://lsg.musin.de",
	"bugs": {
		"url": "https://github.com/3x071c/lsg-stack/issues"
	},
	"author": {
		"email": "dev@3x071c.com",
		"name": "Victor Homic",
		"url": "https://3x071c.com"
	},
	"license": "AGPL-3.0-or-later",
	"private": true,
	"engines": {
		"node": ">= 14"
	},
	"os": [
		"linux",
		"darwin",
		"win64",
		"win32"
	],
	"scripts": {
		"analyze": "cross-env ANALYZE=true npm run build",
		"apollo:generate": "apollo client:codegen --includes=\"{app,graphql,lib,pages}/**\" --localSchemaFile=__generated__/schema.graphql --outputFlat --tagName=gql --target=typescript --tsFileExtension=d.ts --useReadOnlyTypes __generated__/apollo.d.ts",
		"apollo:watch": "apollo client:codegen --includes=\"{app,graphql,lib,pages}/**\" --localSchemaFile=__generated__/schema.graphql --outputFlat --tagName=gql --target=typescript --tsFileExtension=d.ts --useReadOnlyTypes --watch __generated__/apollo.d.ts",
		"build": "cross-env NODE_ENV=production run-s clean generate next:build",
		"check": "tsc --noEmit",
		"ci": "run-s new lint check build",
		"clean": "run-s \"clean:*\"",
		"clean:1": "rimraf .next",
		"commit": "run-s stage cz",
		"cspell:individual": "cspell",
		"cspell:report": "npm run cspell:individual -- \"**\"",
		"cz": "cz",
		"dev": "npm run development",
		"development": "cross-env NODE_ENV=development run-s clean generate next:dev",
		"eslint:fix": "npm run eslint:template -- --fix .",
		"eslint:individual": "npm run eslint:template -- --fix",
		"eslint:report": "npm run eslint:template -- --max-warnings 0 .",
		"eslint:template": "eslint --cache --report-unused-disable-directives --no-error-on-unmatched-pattern --ext .tsx,.ts,.jsx,.js,.mjs,.cjs",
		"export": "cross-env NODE_ENV=production run-s clean next:export",
		"fix": "npm run format",
		"format": "run-s prettier:fix eslint:fix stylelint:fix secretlint:report cspell:report jscpd:report",
		"generate": "run-s prisma:generate nexus:generate apollo:generate",
		"jscpd:individual": "jscpd",
		"jscpd:report": "npm run jscpd:individual -- \".\"",
		"lint": "run-s prettier:report eslint:report stylelint:report secretlint:report cspell:report jscpd:report",
		"new": "run-s clean prisma:reset prisma:push generate prisma:seed",
		"next:build": "next build",
		"next:dev": "next dev",
		"next:export": "next export",
		"next:start": "next start",
		"nexus:generate": "cross-env NODE_ENV=development ts-node --transpile-only graphql/schema",
		"nexus:watch": "cross-env NODE_ENV=development ts-node-dev --transpile-only graphql/schema",
		"preinstall": "npx only-allow npm",
		"prepare": "npx husky install",
		"prettier:fix": "npm run prettier:individual -- .",
		"prettier:individual": "npm run prettier:template -- --write",
		"prettier:report": "npm run prettier:template -- --check .",
		"prettier:template": "prettier --plugin-search-dir=. --ignore-unknown",
		"prisma:deploy": "prisma migrate deploy",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev --skip-generate --skip-seed --name \"name\"",
		"prisma:push": "prisma db push --skip-generate",
		"prisma:reset": "prisma migrate reset --skip-generate --skip-seed --force",
		"prisma:seed": "prisma db seed",
		"prisma:studio": "prisma studio",
		"prisma:watch": "npm run prisma:generate -- --watch",
		"prod": "npm run production",
		"production": "cross-env NODE_ENV=production run-s build start",
		"report": "npm run lint",
		"secretlint:individual": "secretlint",
		"secretlint:report": "npm run secretlint:individual -- \".\"",
		"stage": "git add -A",
		"start": "cross-env NODE_ENV=production npm run next:start",
		"stylelint:fix": "npm run stylelint:template -- --fix \"./**/*.{tsx,jsx,html,pcss,css}\"",
		"stylelint:individual": "npm run stylelint:template -- --fix",
		"stylelint:report": "npm run stylelint:template -- --max-warnings 0 \"./**/*.{tsx,jsx,html,pcss,css}\"",
		"stylelint:template": "stylelint --cache --report-needless-disables --report-invalid-scope-disables --report-descriptionless-disables",
		"watch": "run-p prisma:watch nexus:watch apollo:watch"
	},
	"dependencies": {
		"@apollo/client": "3.5.9",
		"@chakra-ui/icons": "1.1.7",
		"@chakra-ui/react": "1.8.5",
		"@chakra-ui/system": "1.11.1",
		"@emotion/cache": "11.7.1",
		"@emotion/css": "11.7.1",
		"@emotion/react": "11.8.1",
		"@emotion/server": "11.4.0",
		"@emotion/styled": "11.8.1",
		"@next/bundle-analyzer": "12.1.0",
		"@prefresh/babel-plugin": "0.4.3",
		"@prisma/client": "3.10.0",
		"apollo-server-micro": "3.6.3",
		"argon2": "0.28.4",
		"framer-motion": "6.2.8",
		"graphql": "15.8.0",
		"graphql-scalars": "1.14.1",
		"iron-session": "6.0.5",
		"lodash": "4.17.21",
		"micro": "9.3.4",
		"micro-cors": "0.1.1",
		"next": "12.1.0",
		"next-compose-plugins": "2.2.1",
		"next-plugin-preact": "3.0.6",
		"nexus": "1.2.0",
		"preact": "10.6.6",
		"preact-render-to-string": "5.1.20",
		"preact-ssr-prepass": "1.2.0",
		"react": "npm:@preact/compat@17.0.3",
		"react-dom": "npm:@preact/compat@17.0.3",
		"react-hook-form": "7.27.1",
		"react-icons": "4.3.1",
		"react-ssr-prepass": "npm:preact-ssr-prepass@1.2.0",
		"react-table": "7.7.0",
		"sharp": "0.30.1",
		"webpack": "5.69.1",
		"yup": "0.32.11"
	},
	"devDependencies": {
		"@babel/core": "7.17.5",
		"@commitlint/cli": "16.2.1",
		"@commitlint/config-conventional": "16.2.1",
		"@cspell/dict-de-de": "2.1.0",
		"@faker-js/faker": "6.0.0-alpha.7",
		"@secretlint/secretlint-rule-preset-recommend": "4.1.4",
		"@stylelint/postcss-css-in-js": "0.37.2",
		"@types/faker": "5.5.9",
		"@types/lodash": "4.14.179",
		"@types/micro": "7.3.6",
		"@types/micro-cors": "0.1.2",
		"@types/node": "17.0.21",
		"@types/react": "17.0.39",
		"@types/react-dom": "17.0.11",
		"@typescript-eslint/eslint-plugin": "5.12.1",
		"@typescript-eslint/parser": "5.12.1",
		"apollo": "2.33.9",
		"autoprefixer": "10.4.2",
		"browserslist": "4.19.3",
		"commitizen": "4.2.4",
		"cross-env": "7.0.3",
		"cspell": "5.18.5",
		"cssnano": "5.0.17",
		"cssnano-preset-advanced": "5.1.12",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "8.10.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "16.1.0",
		"eslint-config-next": "12.1.0",
		"eslint-config-prettier": "8.4.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.29.2",
		"eslint-plugin-react-hooks": "4.3.0",
		"faker": "5.5.3",
		"husky": "7.0.4",
		"jscpd": "3.4.5",
		"lint-staged": "12.3.4",
		"npm-run-all": "4.1.5",
		"only-allow": "1.1.0",
		"postcss": "8.4.7",
		"postcss-fail-on-warn": "0.2.1",
		"postcss-flexbugs-fixes": "5.0.2",
		"postcss-import": "14.0.2",
		"postcss-preset-env": "7.4.1",
		"postcss-syntax": "0.36.2",
		"prettier": "2.5.1",
		"prettier-plugin-pkg": "0.11.1",
		"prettier-plugin-prisma": "3.10.0",
		"prettier-plugin-sh": "0.8.1",
		"prettier-plugin-sort-json": "0.0.2",
		"prisma": "3.10.0",
		"rimraf": "3.0.2",
		"secretlint": "4.1.4",
		"stylelint": "14.5.3",
		"stylelint-config-prettier": "9.0.3",
		"stylelint-config-recommended": "7.0.0",
		"stylelint-config-standard": "25.0.0",
		"ts-node": "10.5.0",
		"ts-node-dev": "1.1.8",
		"tsconfig-paths": "3.12.0",
		"typescript": "4.5.5"
	},
	"browserslist": [
		"defaults"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"npm run prettier:individual",
			"npm run secretlint:individual",
			"npm run cspell:individual",
			"npm run jscpd:individual"
		],
		"*.{tsx,jsx,html,pcss,css}": "npm run stylelint:individual",
		"*.{tsx,ts,jsx,js,mjs,cjs}": "npm run eslint:individual"
	},
	"prisma": {
		"schema": "db/schema.prisma",
		"seed": "ts-node db/seed.ts"
	}
}
