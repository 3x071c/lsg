{
	"name": "lsg",
	"version": "0.0.0",
	"description": "LSG musin fullstack",
	"repository": {
		"type": "git",
		"url": "https://github.com/3x071c/lsg"
	},
	"homepage": "https://lsg.musin.de",
	"bugs": {
		"url": "https://github.com/3x071c/lsg/issues"
	},
	"author": {
		"email": "dev@3x071c.com",
		"name": "Victor Homic",
		"url": "https://3x071c.com"
	},
	"license": "AGPL-3.0-or-later",
	"private": true,
	"engines": {
		"node": ">= 16"
	},
	"os": [
		"linux",
		"darwin"
	],
	"scripts": {
		"build": "cross-env NODE_ENV=production run-s \"build:*\"",
		"build:1": "npm run remix:build",
		"build:2": "npm run server:build",
		"check": "tsc --noEmit",
		"clean": "run-s \"clean:*\"",
		"clean:1": "rimraf .remix",
		"clean:2": "rimraf public/build",
		"commit": "run-s stage cz",
		"cspell:individual": "cspell",
		"cspell:report": "npm run cspell:individual -- \"**\" \".**\"",
		"cz": "cz",
		"dev": "npm run development",
		"development": "cross-env NODE_ENV=development run-s \"development:*\"",
		"development:1": "npm run clean",
		"development:2": "npm run remix:build",
		"development:3": "run-p \"development:3:*\"",
		"development:3:remix": "npm run remix:dev",
		"development:3:server": "nodemon --inspect --require dotenv/config .remix/server/server.js",
		"eslint:fix": "npm run eslint:template -- --fix .",
		"eslint:individual": "npm run eslint:template -- --fix",
		"eslint:report": "npm run eslint:template -- --max-warnings 0 .",
		"eslint:template": "eslint --cache --report-unused-disable-directives --no-error-on-unmatched-pattern --ext .tsx,.ts,.jsx,.js,.mjs,.cjs --ignore-path .gitignore",
		"fix": "npm run format",
		"format": "run-s prettier:fix eslint:fix secretlint:report cspell:report jscpd:report",
		"generate": "npm run prisma:generate",
		"jscpd:individual": "jscpd",
		"jscpd:report": "npm run jscpd:individual -- \".\"",
		"lint": "run-s prettier:report eslint:report secretlint:report cspell:report jscpd:report",
		"new": "run-s generate prisma:reset prisma:dev prisma:seed",
		"postinstall": "remix setup express",
		"preinstall": "npx --yes only-allow npm",
		"prepare": "husky install",
		"prettier:fix": "npm run prettier:individual -- .",
		"prettier:individual": "npm run prettier:template -- --write",
		"prettier:report": "npm run prettier:template -- --check .",
		"prettier:template": "prettier --plugin-search-dir=. --ignore-unknown --ignore-path .gitignore",
		"prisma:dev": "prisma migrate dev --skip-generate --skip-seed",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev --skip-generate --skip-seed --name",
		"prisma:prod": "prisma migrate deploy",
		"prisma:push": "prisma db push --skip-generate",
		"prisma:reset": "prisma migrate reset --skip-generate --skip-seed --force",
		"prisma:seed": "prisma db seed",
		"prisma:studio": "prisma studio",
		"prisma:watch": "npm run prisma:generate -- --watch",
		"prod": "npm run production",
		"production": "cross-env NODE_ENV=production run-s clean build start",
		"release": "npm run prisma:prod",
		"remix:build": "remix build",
		"remix:dev": "remix watch",
		"remix:start": "node ./server.js",
		"report": "npm run lint",
		"secretlint:individual": "secretlint --secretlintignore .gitignore",
		"secretlint:report": "npm run secretlint:individual -- \".\"",
		"server:build": "esbuild --platform=node --format=cjs app/server.ts --outdir=.remix/server",
		"server:watch": "npm run server:build -- --watch",
		"stage": "git add -A",
		"start": "cross-env NODE_ENV=production npm run remix:start"
	},
	"dependencies": {
		"@chakra-ui/icons": "1.1.7",
		"@chakra-ui/react": "1.8.8",
		"@chakra-ui/system": "1.12.1",
		"@chakra-ui/theme-tools": "1.3.6",
		"@charlietango/use-native-lazy-loading": "1.10.0",
		"@emotion/cache": "11.7.1",
		"@emotion/react": "11.9.0",
		"@emotion/server": "11.4.0",
		"@emotion/styled": "11.8.1",
		"@emotion/utils": "1.1.0",
		"@magic-sdk/admin": "1.4.0",
		"@prisma/client": "3.12.0",
		"@remix-run/express": "1.4.0",
		"@remix-run/node": "1.4.0",
		"@remix-run/react": "1.4.0",
		"@remix-run/serve": "1.4.0",
		"@remix-run/server-runtime": "1.4.0",
		"@remix-validated-form/with-zod": "2.0.1",
		"compression": "1.7.4",
		"cross-env": "7.0.3",
		"express": "4.17.3",
		"framer-motion": "6.3.0",
		"lodash": "4.17.21",
		"magic-sdk": "8.1.0",
		"morgan": "1.10.0",
		"npm-run-all": "4.1.5",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-focus-lock": "2.8.1",
		"react-hook-form": "7.29.0",
		"react-icons": "4.3.1",
		"react-intersection-observer": "8.34.0",
		"react-table": "7.7.0",
		"react-window": "1.8.6",
		"remix": "1.4.0",
		"remix-validated-form": "4.3.0",
		"superjson": "1.8.1",
		"zod": "3.14.4"
	},
	"devDependencies": {
		"@babel/core": "7.17.9",
		"@commitlint/cli": "16.2.3",
		"@commitlint/config-conventional": "16.2.1",
		"@cspell/dict-de-de": "2.1.0",
		"@faker-js/faker": "6.1.2",
		"@remix-run/dev": "1.4.0",
		"@remix-run/eslint-config": "1.4.0",
		"@secretlint/secretlint-rule-preset-recommend": "5.2.0",
		"@swc/core": "1.2.165",
		"@types/compression": "1.7.2",
		"@types/eslint": "8.4.1",
		"@types/express": "4.17.13",
		"@types/morgan": "1.9.3",
		"@types/node": "17.0.24",
		"@types/react": "17.0.44",
		"@types/react-dom": "17.0.15",
		"@types/react-table": "7.7.10",
		"@types/react-window": "1.8.5",
		"@typescript-eslint/eslint-plugin": "5.19.0",
		"@typescript-eslint/parser": "5.19.0",
		"browserslist": "4.20.2",
		"commitizen": "4.2.4",
		"cspell": "5.19.7",
		"cz-conventional-changelog": "3.3.0",
		"dotenv": "16.0.0",
		"esbuild": "0.14.36",
		"eslint": "8.13.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.29.4",
		"eslint-plugin-react-hooks": "4.4.0",
		"husky": "7.0.4",
		"jscpd": "3.4.5",
		"nodemon": "2.0.15",
		"only-allow": "1.1.0",
		"prettier": "2.6.2",
		"prettier-plugin-pkg": "0.13.0",
		"prettier-plugin-prisma": "3.12.0",
		"prettier-plugin-sh": "0.10.1",
		"prettier-plugin-sort-json": "0.0.2",
		"prisma": "3.12.0",
		"rimraf": "3.0.2",
		"secretlint": "5.2.0",
		"ts-node": "10.7.0",
		"tsconfig-paths": "3.14.1",
		"typescript": "4.6.3"
	},
	"sideEffects": false,
	"browserslist": [
		"defaults"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"prisma": {
		"schema": "db/schema.prisma",
		"seed": "ts-node db/seed.ts"
	}
}
